{
  "description": "Adds a browser action icon to the toolbar. Click the button to attempt to open link in sci-hub or LibGen.",
  "name": "Break Down the Walls",
  "manifest_version": 2,
  "version": "1.2.6",
  "icons": {
    "48": "graduate-hat.png"
  },
  "background": {
    "scripts": [
      "scihubify.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "elementSelectionScript.js"
      ]
    }
  ],
  "web_accessible_resources": [
    "elementSelectionScript.js"
  ],
  "browser_action": {
    "browser_style": true,
    "default_icon": "graduate-hat.png",
    "default_title": "Break Down Walls"
  },
  "permissions": [
    "<all_urls>",
    "tabs",
    "storage",
    "activeTab",
    "menus",
    "notifications"
  ],
  "optional_permissions": [
    "menus"
  ],
  "options_ui": {
    "page": "preferences.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "onurhanak@protonmail.com",
      "strict_min_version": "48.0"
    }
  }
}